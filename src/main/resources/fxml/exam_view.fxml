<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.control.Pagination?>
<?import java.net.URL?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="navigation.controller.ExamController"
            prefHeight="400.0" prefWidth="600.0">
    <stylesheets>
        <URL value="@/css/exam_css.css"></URL>
    </stylesheets>
    <left>
        <VBox fx:id="leftBox" alignment="CENTER">
            <padding>
                <Insets>
                    <top>50.0</top>
                    <bottom>50.0</bottom>
                    <left>10.0</left>
                    <right>10.0</right>
                </Insets>
            </padding>
            <VBox alignment="CENTER" spacing="10.0">
                <HBox alignment="CENTER">
                <ImageView>
                    <image>
                        <Image url="@/images/icon.png" requestedWidth="70.0" requestedHeight="70.0"></Image>
                    </image>
                </ImageView>
                </HBox>
                <HBox alignment="CENTER">
                    <Label text="航海类教育考试系统" fx:id="title"></Label>
                </HBox>
            </VBox>
        </VBox>
    </left>
    <center>
            <Pagination fx:id="pages" pageCount="2">

            </Pagination>
    </center>

</BorderPane>
